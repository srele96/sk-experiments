{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "jsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        // Fast refresh won't work without explicitly setting development: true.
        "development": true,
        "refresh": true
      }
    }
  }
}
